<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>Silva SMI</title>
    <meta name="generator" content="Silva" />
    <tal:resource tal:replace="structure provider:resources" />
    <base href="#" target="_parent"
      tal:attributes="href string:${context/absolute_url}/edit" />
    
    <link href="#"
      rel="stylesheet"
      type="text/css"
      tal:condition="python: not context.implements_container()"
      tal:attributes="href string:${layout/root_url}/globals/silvaContentStyle.css"
    />
    <link rel="shortcut icon"
          tal:attributes="href string:${layout/resource_base_url}/silvacon.ico" />
  </head>
  <body onload="window.onload_registry.callHandlers()">
    <tal:header tal:content="structure provider:header" />
    <div class="workspace">
      <tal:replace tal:replace="structure provider:smimiddlegroundmanager" />
      <table class="columns columns2">
        <tr>
          <td class="left">
            <tal:navigation tal:content="structure provider:navigation" />
          </td>
          <td class="main">
            <tal:content tal:content="structure view/content" />
          </td>
        </tr>
      </table>
    </div>
    <tal:footer tal:content="structure provider:footer" />
  </body>
</html>
