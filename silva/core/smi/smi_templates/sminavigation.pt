<table class="listing" cellspacing="0"
    style="border-collapse: separate; margin-top: -1px"
    i18n:domain="silva">
  <thead>
    <tr>
      <th class="top">
        <a class="up"
          title="up one level: alt-]"
          accesskey="]"
          tal:condition="not: provider/is_root"
          tal:attributes="href string:provider/up_url"
          i18n:attributes="title">
          <img width="9"
            height="9"
            border="0"
            alt="up one level: alt-]"
            i18n:attributes="alt"
            tal:attributes="src string:${layout/resource_base_url}/up_level.gif" />
        </a>
        <a class="top"
          title="up to containing publication: alt-["
          accesskey="["
          i18n:attributes="title"
          tal:condition="not: provider/is_root"
          tal:attributes="href provider/top_url">
          <img width="9"
            height="9"
            border="0"
            alt="up to containing publication: alt-["
            i18n:attributes="alt"
            tal:attributes="src provider/top_image_src" />
        </a>
        <a class="normal"
          title="reload screen"
          tal:content="provider/vein"
          tal:attributes="href layout/view_name"
          i18n:translate=""
          i18n:attributes="title">
          vein
        </a>
      </th>
    </tr>
  </thead>
  <tbody tal:content="structure provider:navigation_listing" />
</table>