<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html tal:attributes="lang layout/lang | nothing;
                      xml:lang layout/lang | nothing">
  <head>
    <title tal:content="context/get_title_or_id">Silva SMI</title>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta name="generator" content="Silva" />
    <tal:resource tal:replace="structure provider:resources" />
    <base href="#" target="_parent"
          tal:attributes="href string:${context/@@absolute_url}/edit" />
    <tal:favicon tal:replace="structure provider:favicon" />
  </head>
  <body>
    <tal:header tal:content="structure provider:header" />
    <div class="workspace">
      <tal:replace tal:replace="structure provider:smimiddlegroundmanager" />
      <tal:replace tal:replace="structure provider:smimessages" />
      <div class="middlespace">
        <div class="navigation"
             tal:content="structure provider:sminavigation">
        </div>
        <div class="viewport"
             tal:content="structure view/content">
        </div>
        <div class="portlets"
             tal:content="structure provider:smiportletmanager">
        </div>
        <div class="viewport-clear"></div>
      </div>
    </div>
    <tal:footer tal:content="structure provider:footer" />
    <div id="remote-form-dialog">
    </div>
    <div id="remote-request-loading">
      <p i18n:translate="">loading ...</p>
    </div>
    <a href="#" id="content-url" style="display: none"
       tal:attributes="href context/@@absolute_url"></a>
  </body>
</html>
