<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html>
  <head>
    <title>Silva SMI</title>
    <meta name="generator" content="Silva" />
    <tal:resource tal:replace="structure provider:resources" />
    <base href="#" target="_parent"
          tal:attributes="href string:${context/@@absolute_url}/edit" />
    <link href="#"
      rel="stylesheet"
      type="text/css"
      tal:condition="python: not context.implements_container()"
      tal:attributes="href string:${layout/root_url}/globals/silvaContentStyle.css"
    />
    <link rel="shortcut icon"
          tal:attributes="href layout/static/silvacon.ico" />
  </head>
  <body>
    <tal:header tal:content="structure provider:header" />
    <div class="workspace">
      <tal:replace tal:replace="structure provider:smimiddlegroundmanager" />
      <tal:replace tal:replace="structure provider:smimessages" />
      <div class="middlespace">
        <div class="navigation"
             tal:content="structure provider:sminavigation">
        </div>
        <div class="viewport"
             tal:content="structure view/content">
        </div>
        <div class="portlets"
             tal:content="structure provider:smiportletmanager">
        </div>
        <div class="viewport-clear"></div>
      </div>
    </div>
    <tal:footer tal:content="structure provider:footer" />
    <div id="remote-form-dialog">
    </div>
  </body>
</html>
