<tal:error i18n:domain="silva">
  <h1 i18n:translate="">Over Quota</h1>
  <p class="disclaimer" i18n:translate="">
    Sorry, a quota is applied to the container you are working in, and the
    current operation would require <b i18n:name="exceeding_size"
    tal:content="view.exceeding_size">1.2MB</b> more space.
  </p>
  <p class="disclaimer">
    <a tal:attributes="href view.next_url"
      i18n:translate="">Return to the previous screen...</a>
  </p>
</tal:error>
